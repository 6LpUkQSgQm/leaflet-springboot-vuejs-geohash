type GeohashObject {
  id: String!
  countryCode: String!
  name: String!
  latitude: String!
  longitude: String!
  geohashValue: String!
  createdDate: String!
}

# Root
type Query {
  findAllGeohashs: [GeohashObject]!
  findGeohash(id: String): GeohashObject
}

# Root
type Mutation {
  createGeohash(
    countryCode: String!
    name: String!
    latitude: String!
    longitude: String!
  ): GeohashObject!
}
