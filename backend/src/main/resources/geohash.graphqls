type Geohash {
  id: String!
  countryCode: String!
  name: String!
  latitude: String!
  longitude: String!
  geohashValue: String!
  createdDate: String!
}

# Root
type Query {
  findAllGeohashs: [Geohash]!
  findGeohash(id: String): Geohash
}

# Root
type Mutation {
  createGeohash(
    countryCode: String!
    name: String!
    latitude: String!
    longitude: String!
  ): Geohash!
}
