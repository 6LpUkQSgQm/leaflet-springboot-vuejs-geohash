type Geohash {
  id: String!
  name: String!
  latitude: String!
  longitude: String!
  geohashValue: String!
}

# Root
type Query {
  findAllGeohashs: [Geohash]!
  findGeohash(id: String): Geohash
}

# Root
type Mutation {
  createGeohash(name: String!, latitude: String!, longitude: String!): Geohash!
}
